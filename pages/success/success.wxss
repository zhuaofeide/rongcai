/* pages/success/success.wxss */
.wrapper{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top:0;
  background: #fff;
  padding:40rpx 20rpx;
}
.msg{
  padding: 40rpx 20rpx;
  text-align: center;
}
.msg-img{
  width: 200rpx;
  height: 200rpx;
}
.text{
  text-align: center;
}
.text-inner{
  font-size: 32rpx;
  line-height: 48rpx;
  color: #333;
}
.btn-box {
  padding: 80rpx 20rpx 0 20rpx;
}

.btn-click {
  background-color: #349fec;
  color: #fff;
  border-radius: 30px;
  font-size: 36rpx;
  width: 85%;
}

.button-hover {
  background-color: #1c95ec;
}