/* pages/resume-detail/resume-detail.wxss */
.resume-box{
  position: fixed;
  width: 100%;
  bottom:0;
  left: 0;
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  background: #fff;
}
.btn-click {
  background-color: #349fec;
  color: #fff;
  border-radius: 30px;
  font-size: 36rpx;
  width:45%;
}
.btn-view{
  background-color: #eee;
  color: #1c95ec;
  border-radius: 30px;
  font-size: 36rpx;
  width:45%;
}
.button-hover {
  background-color: #1c95ec;
}
.btn-view-hover{
    background-color: #ddd;
}