/* pages/import-interview/import-interview.wxss */
.wrapper{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  background: #fff;
}
.content {
  width: 100%;
}

.path-item {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 28%;
  float: left;
  text-align: center;
  margin-left: 4%;
  margin-top: 60rpx;
}
.img-box{
  text-align: center;
}
.item-img {
  width: 120rpx;
  height: 120rpx;
}
.item-text{
  font-size: 32rpx;
  line-height: 48rpx;
  color: #333;
}
